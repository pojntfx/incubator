{
  "name": "@pojntfx-incubator/social-media-feeds-core",
  "version": "0.0.1-0",
  "description": "A simple library to get social media feeds from GitLab, GitHub, Reddit, Facebook or Instagram",
  "main": "dist/index.js",
  "scripts": {
    "dev": "jest --watch --setupFiles dotenv/config",
    "build": "babel src/ -d dist/",
    "test": "jest --setupFiles dotenv/config",
    "prepublishOnly": "npm run build"
  },
  "keywords": [
    "gitlab",
    "github",
    "reddit",
    "facebook",
    "instagram"
  ],
  "author": "Felix Pojtinger <felix@pojtinger.com>",
  "license": "AGPL-3.0",
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-jest": "^23.6.0",
    "babel-preset-env": "^1.7.0",
    "dotenv": "^6.2.0",
    "jest": "^23.6.0",
    "nodemon": "^1.18.9"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "node": "10.14.2"
          }
        }
      ]
    ]
  },
  "dependencies": {
    "insta-api": "^1.2.1",
    "node-fetch": "^2.3.0"
  },
  "publishConfig": {
    "access": "public"
  }
}
